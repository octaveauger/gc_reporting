<%- headers = [
	t('.table_title_creation_date'), 
	t('.table_title_customer_fname'), 
	t('.table_title_customer_lname'), 
	t('.table_title_company_name'), 
	t('.table_title_email'), 
	t('.table_title_source')] -%>
<%= CSV.generate_line headers, :row_sep => ?\t, :quote_char => ?\ %>
<%- @clients.each do |client| -%>
  <%= CSV.generate_line [
	  		client.created_at.strftime('%d/%m/%Y  %k:%M'),
	  		client.fname,
	  		client.lname,
	  		client.company_name,
	  		client.email,
	  		client.client_source.display_name], 
	  	:row_sep => ?\t, :quote_char => ?\ %>
<%- end -%>